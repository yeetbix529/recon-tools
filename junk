"value":"https://syndication.nba.com",
"status_code":200,
"in_scope":true,
"technologies":["Akamai"],
"dns": {
	"a": ["104.85.238.48"],
	"cname": ["delaware.gleague.nba.com.edgekey.net",
	"e1503.dsci.akamaiedge.net"
	],
	
	"ttl": 300,
	"resolver": ["1.1.1.1:53"],
	"timestamp": "2025-06-03T21:41:48.501229869+10:00"
	}
}

